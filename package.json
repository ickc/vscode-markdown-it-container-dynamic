{
  "name": "vscode-markdown-it-container-dynamic",
  "version": "0.1.0",
  "displayName": "Markdown Dynamic Container",
  "description": "Plugin for creating block-level dynamic containers for markdown-it markdown parser.",
  "keywords": [
    "markdown",
    "markdown-it",
    "markdown-it-plugin",
    "pandoc"
  ],
  "publisher": "ickc",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/ickc/vscode-markdown-it-container-dynamic.git"
  },
  "bugs": {
    "url": "https://github.com/ickc/vscode-markdown-it-container-dynamic/issues"
  },
  "engines": {
    "vscode": "^1.33.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:markdown"
  ],
  "contributes": {
    "markdown.markdownItPlugins": true
  },
  "main": "./extension.js",
  "devDependencies": {
    "@types/vscode": "^1.33.0"
  },
  "dependencies": {
    "markdown-it-container": "^3.0.0"
